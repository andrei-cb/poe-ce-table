  <LuaScript>function poeattach(timer)
  if getProcessIDFromProcessName("PathofExileSteam.exe") ~= nil then
    object_destroy(timer)
    openProcess("PathofExileSteam.exe")
  end
end

t=createTimer(nil);
timer_setInterval(t,10)
timer_onTimer(t,poeattach)
</LuaScript>
